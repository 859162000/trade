<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd">

	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="600" overflowToDisk="false">
	</defaultCache>

	<cache name="guidCache" maxElementsInMemory="500000" eternal="false" timeToIdleSeconds="7200" timeToLiveSeconds="7200" overflowToDisk="false" />
	
	<cache name="countryCacheName" maxElementsInMemory="500000" eternal="false" timeToIdleSeconds="7200" timeToLiveSeconds="7200" overflowToDisk="false" /><!--GuideDeliverService getDeliverGuidByCityGuideLevel  -->
	<cache name="countGuide" maxElementsInMemory="500000" eternal="false" timeToIdleSeconds="7200" timeToLiveSeconds="7200" overflowToDisk="false" />
	<cache name="cityCache" maxElementsInMemory="500000" eternal="false" timeToIdleSeconds="10200" timeToLiveSeconds="10200" overflowToDisk="false" />
	<cache name="channelCache" maxElementsInMemory="500000" eternal="false" timeToIdleSeconds="10200" timeToLiveSeconds="10200" overflowToDisk="false" />
	<cache name="airportCache" maxElementsInMemory="500000" eternal="false" timeToIdleSeconds="10200" timeToLiveSeconds="10200" overflowToDisk="false" />
	
	<cache name="confCache" maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="10200" timeToLiveSeconds="10200" overflowToDisk="false" />
</ehcache>