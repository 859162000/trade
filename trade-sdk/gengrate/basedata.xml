<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">


<generatorConfiguration>
	<!-- classPathEntry:数据库的JDBC驱动 -->
	<classPathEntry location="D:\dev\hbc\sourcecode\mybatis-generator-usage\lib\mysql-connector-java-5.1.6-bin.jar" />
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.MySQLPaginationPlugin" >
			<property name="pageClassPath" value="com.hbc.api.trade.bdata.common.Page"/>
		</plugin>
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
		<!-- 去除自动生成的注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="false" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.1.7:3306/hbc_new" userId="cclx"
			password="cclx123456">
		</jdbcConnection>


		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- targetProject:自动生成代码的位置 -->
		<javaModelGenerator targetPackage="com.hbc.api.trade.bdata.mapper.basedata.gen.bean"
			targetProject="D:\dev\hbc\svn\java\trade\trunk\trade-sdk\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>


		<sqlMapGenerator targetPackage="com.hbc.api.trade.bdata.mapper.basedata.gen"
			targetProject="D:\dev\hbc\svn\java\trade\trunk\trade-sdk\src\main\java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>


		<javaClientGenerator type="ANNOTATEDMAPPER"
			targetPackage="com.hbc.api.trade.bdata.mapper.basedata.gen" targetProject="D:\dev\hbc\svn\java\trade\trunk\trade-sdk\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="exampleMethodVisibility" value="false" />
		</javaClientGenerator>
		<!-- tableName:用于自动生成代码的数据库表；domainObjectName:对应于数据库表的javaBean类名 -->
		
		<table schema="" tableName="basedata_associated_cities" domainObjectName="AssociatedCity"></table>
		<table schema="" tableName="basedata_cities" domainObjectName="CityBean"></table>
		<table schema="" tableName="basedata_airport" domainObjectName="AirportBean"></table>
		<table schema="" tableName="basedata_countries" domainObjectName="CountryBean"></table>
		<table schema="" tableName="basedata_continents" domainObjectName="ContinentBean"></table>
		<table schema="" tableName="basedata_car" domainObjectName="Car"></table>
		<table schema="" tableName="user_fund_account" domainObjectName="UserFundAccount"></table>
		<table schema="" tableName="user_info" domainObjectName="UserInfo"></table>
	</context>

</generatorConfiguration>
